pipeline{
agent any 
tools{
Maven "mvn"
}
stages{

stage('clean and build'){
steps{
       sh 'mvn clean'
       sh 'mvn install'
}

}

stage("SonarQube analysis") {
       
            steps {
              withSonarQubeEnv('sonarqube') {
                sh 'mvn sonar:sonar'
              }
            }
          }
  
       
      
}
}
